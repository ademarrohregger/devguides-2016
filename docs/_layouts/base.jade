doctype html
html
  head
    meta(charset='utf-8')
    meta(name='viewport' content='width=device-width')
    link(rel='stylesheet' href=(base + '/assets/style.css'))
    if next
      link(rel='prefetch' href=next)
      link(rel='prerender' href=next)
      link(rel='next' href=next)
    title
      block title
    block head
  block body
